<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<DialogPane expanded="true" headerText="Finish Workout" stylesheets="@dark-theme.css" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1">
   <header>
      <AnchorPane />
   </header>
   <buttonTypes>
      <ButtonType fx:constant="CANCEL" />
      <ButtonType fx:constant="OK" />
   </buttonTypes>
   <content>
      <AnchorPane prefHeight="308.0" prefWidth="432.0">
         <children>
            <ChoiceBox fx:id="dialogChoiceBox" layoutX="29.0" layoutY="30.0" prefHeight="26.0" prefWidth="173.0" />
            <TableView fx:id="dialogTable" editable="true" layoutX="29.0" layoutY="67.0" prefHeight="224.0" prefWidth="374.0">
              <columns>
                <TableColumn fx:id="dialogExerciseCol" editable="false" prefWidth="242.0" text="Exercise" />
                <TableColumn fx:id="dialogCountCol" prefWidth="105.0" text="Count/Length" />
                  <TableColumn fx:id="dialogCloseCol" editable="false" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefWidth="25.0" resizable="false" sortable="false" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Spinner fx:id="dialogSpinner" editable="true" layoutX="215.0" layoutY="30.0" prefHeight="26.0" prefWidth="103.0" />
            <Button fx:id="dialogAddButton" layoutX="333.0" layoutY="30.0" mnemonicParsing="false" onAction="#addExercise" text="Add" />
         </children>
      </AnchorPane>
   </content>
</DialogPane>
